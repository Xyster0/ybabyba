-- –û–±—Ä–∞–±–æ—Ç—á–∏–∫–∏ –∞–≤—Ç–æ–ø–æ–¥–±–æ—Ä–∞
autoCompleteSearchButton.MouseButton1Click:Connect(function()
    local success = autoCompletePlayerName(searchBox)
    if success then
        autoCompleteSearchButton.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
        wait(0.1)
        autoCompleteSearchButton.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
        
        -- –û–±–Ω–æ–≤–ª—è–µ–º targetPlayerName
        targetPlayerName = searchBox.Text
        teleportAll = false
        button.Text = "–í–ö–õ –¢–µ–ª–µ–ø–æ—Ä—Ç –í–´–ë–†–ê–ù–ù–´–•"
        statusLabel.Text = "–¶–µ–ª—å: " .. targetPlayerName
    else
        autoCompleteSearchButton.BackgroundColor3 = Color3.fromRGB(80, 30, 30)
        wait(0.2)
        autoCompleteSearchButton.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
    end
end)

autoCompleteIgnoreButton.MouseButton1Click:Connect(function()
    local success = autoCompletePlayerName(ignoreBox)
    if success then
        autoCompleteIgnoreButton.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
        wait(0.1)
        autoCompleteIgnoreButton.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
        
        -- –û–±–Ω–æ–≤–ª—è–µ–º ignorePlayerName
        ignorePlayerName = ignoreBox.Text
    else
        autoCompleteIgnoreButton.BackgroundColor3 = Color3.fromRGB(80, 30, 30)
        wait(0.2)
        autoCompleteIgnoreButton.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
    end
end)

autoCompleteESPButton.MouseButton1Click:Connect(function()
    local success = autoCompletePlayerName(espSearchBox)
    if success then
        autoCompleteESPButton.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
        wait(0.1)
        autoCompleteESPButton.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
        
        -- –û–±–Ω–æ–≤–ª—è–µ–º espTarget
        espTarget = espSearchBox.Text
        if espEnabled then
            espStatusLabel.Text = "–í–•: –í–ö–õ - " .. espTarget
            -- –û–±–Ω–æ–≤–ª—è–µ–º ESP –¥–ª—è –Ω–æ–≤–æ–π —Ü–µ–ª–∏
            for _, otherPlayer in pairs(Players:GetPlayers()) do
                if otherPlayer ~= player then
                    applyESPToPlayer(otherPlayer)
                end
            end
        end
    else
        autoCompleteESPButton.BackgroundColor3 = Color3.fromRGB(80, 30, 30)
        wait(0.2)
        autoCompleteESPButton.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
    end
end)

-- –û—Å—Ç–∞–ª—å–Ω—ã–µ –æ–±—Ä–∞–±–æ—Ç—á–∏–∫–∏
searchBox:GetPropertyChangedSignal("Text"):Connect(function()
    if searchBox.Text == "" then
        teleportAll = true
        targetPlayerName = ""
        button.Text = "–í–ö–õ –¢–µ–ª–µ–ø–æ—Ä—Ç –í–°–ï–•"
    else
        teleportAll = false
        targetPlayerName = searchBox.Text
        button.Text = "–í–ö–õ –¢–µ–ª–µ–ø–æ—Ä—Ç –í–´–ë–†–ê–ù–ù–´–•"
    end
    statusLabel.Text = "–¶–µ–ª—å: " .. (targetPlayerName == "" and "–í—Å–µ" or targetPlayerName)
end)

ignoreBox:GetPropertyChangedSignal("Text"):Connect(function()
    ignorePlayerName = ignoreBox.Text
end)

button.MouseButton1Click:Connect(function()
    enabled = not enabled
    if enabled then
        button.Text = "–í–´–ö–õ –¢–µ–ª–µ–ø–æ—Ä—Ç " .. (teleportAll and "–í–°–ï–•" or "–í–´–ë–†–ê–ù–ù–´–•")
        button.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
        print("SOSISON HUB —Ç–µ–ª–µ–ø–æ—Ä—Ç –≤–∫–ª—é—á–µ–Ω")
    else
        button.Text = "–í–ö–õ –¢–µ–ª–µ–ø–æ—Ä—Ç " .. (teleportAll and "–í–°–ï–•" or "–í–´–ë–†–ê–ù–ù–´–•")
        button.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
        print("SOSISON HUB —Ç–µ–ª–µ–ø–æ—Ä—Ç –≤—ã–∫–ª—é—á–µ–Ω")
    end
end)

-- –û–±—Ä–∞–±–æ—Ç—á–∏–∫–∏ ESP
espSearchBox:GetPropertyChangedSignal("Text"):Connect(function()
    espTarget = espSearchBox.Text
    if espEnabled then
        espStatusLabel.Text = "–í–•: –í–ö–õ - " .. (espTarget == "" and "–í—Å–µ –∏–≥—Ä–æ–∫–∏" or espTarget)
        -- –û–±–Ω–æ–≤–ª—è–µ–º ESP –ø—Ä–∏ –∏–∑–º–µ–Ω–µ–Ω–∏–∏ —Ü–µ–ª–∏
        for _, otherPlayer in pairs(Players:GetPlayers()) do
            if otherPlayer ~= player then
                applyESPToPlayer(otherPlayer)
            end
        end
    end
end)

espAllButton.MouseButton1Click:Connect(function()
    espEnabled = true
    espStatusLabel.Text = "–í–•: –í–ö–õ - " .. (espTarget == "" and "–í—Å–µ –∏–≥—Ä–æ–∫–∏" or espTarget)
    espAllButton.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
    espOffButton.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
    print("SOSISON HUB WH –≤–∫–ª—é—á–µ–Ω")
    
    -- –ù–µ–º–µ–¥–ª–µ–Ω–Ω–æ –ø—Ä–∏–º–µ–Ω—è–µ–º ESP –∫–æ –≤—Å–µ–º –∏–≥—Ä–æ–∫–∞–º
    for _, otherPlayer in pairs(Players:GetPlayers()) do
        if otherPlayer ~= player then
            applyESPToPlayer(otherPlayer)
        end
    end
end)

espOffButton.MouseButton1Click:Connect(function()
    disableAllESP()
    espStatusLabel.Text = "–í–•: –í–´–ö–õ"
    espAllButton.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
    espOffButton.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
    print("SOSISON HUB WH –≤—ã–∫–ª—é—á–µ–Ω")
end)

-- –û–±—Ä–∞–±–æ—Ç—á–∏–∫–∏ FLY –≤–∫–ª–∞–¥–∫–∏
applyFlyButton.MouseButton1Click:Connect(function()
    local newFlySpeed = tonumber(flySpeedBox.Text)
    local newBindKey = flyBindBox.Text:upper()
    
    if newFlySpeed then
        flySpeed = newFlySpeed
        flySpeedLabel.Text = "–°–∫–æ—Ä–æ—Å—Ç—å –ø–æ–ª–µ—Ç–∞: " .. newFlySpeed
    end
    
    if newBindKey ~= "" then
        flyBindKey = newBindKey
        flyBindLabel.Text = "–ö–ª–∞–≤–∏—à–∞ –ø–æ–ª–µ—Ç–∞: " .. flyBindKey
    end
    
    print("SOSISON HUB –Ω–∞—Å—Ç—Ä–æ–π–∫–∏ –ø–æ–ª–µ—Ç–∞ –ø—Ä–∏–º–µ–Ω–µ–Ω—ã")
end)

flyButton.MouseButton1Click:Connect(function()
    if isFlying then
        stopFlying()
        flyButton.Text = "–í–ö–õ –ü–û–õ–ï–¢"
        flyButton.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
        flyStatusLabel.Text = "–ü–æ–ª–µ—Ç: –í–´–ö–õ"
        print("üõ¨ –†–ï–ñ–ò–ú –ü–û–õ–ï–¢–ê –û–¢–ö–õ–Æ–ß–ï–ù!")
    else
        startFlying()
        flyButton.Text = "–í–´–ö–õ –ü–û–õ–ï–¢"
        flyButton.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
        flyStatusLabel.Text = "–ü–æ–ª–µ—Ç: –í–ö–õ"
        print("üõ´ –†–ï–ñ–ò–ú –ü–û–õ–ï–¢–ê –ê–ö–¢–ò–í–ò–†–û–í–ê–ù!")
    end
end)

-- –û–±—Ä–∞–±–æ—Ç—á–∏–∫–∏ MORE –≤–∫–ª–∞–¥–∫–∏
