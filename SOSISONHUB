-- SOSISON HUB - –°–∫—Ä–∏–ø—Ç —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏–∏ —Å –ø–µ—Ä–µ–º–µ—â–∞–µ–º—ã–º GUI
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local UserInputService = game:GetService("UserInputService")

local player = Players.LocalPlayer
local enabled = false
local targetPlayerName = ""
local ignorePlayerName = ""
local teleportAll = true
local espEnabled = false
local espTarget = ""
local guiVisible = true

-- –ù–∞—Å—Ç—Ä–æ–π–∫–∏ —Å–∫–æ—Ä–æ—Å—Ç–∏
local originalWalkSpeed = 50
local originalJumpPower = 50
local customWalkSpeed = 50
local customJumpPower = 50
local moveDirection = Vector3.new(0, 0, 0)
local speedApplied = false
local lastInputTime = 0

-- –ù–∞—Å—Ç—Ä–æ–π–∫–∏ –ø–æ–ª–µ—Ç–∞
local flySpeed = 100
local isFlying = false
local flyBodyVelocity = nil
local flyConnection = nil
local flyBindKey = "U" -- –ö–ª–∞–≤–∏—à–∞ –ø–æ —É–º–æ–ª—á–∞–Ω–∏—é –¥–ª—è –ø–æ–ª–µ—Ç–∞

print("SOSISON HUB –∑–∞–≥—Ä—É–∂–µ–Ω")

-- –§—É–Ω–∫—Ü–∏—è —Å–±—Ä–æ—Å–∞ –¥–≤–∏–∂–µ–Ω–∏—è
local function resetMovement()
    moveDirection = Vector3.new(0, 0, 0)
    
    local char = player.Character
    if char and char:FindFirstChild("HumanoidRootPart") then
        local bv = char.HumanoidRootPart:FindFirstChild("RePozSpeed")
        if bv then
            bv:Destroy()
        end
    end
end

-- –û–±—Ä–∞–±–æ—Ç—á–∏–∫–∏ —Ñ–æ–∫—É—Å–∞ –æ–∫–Ω–∞
UserInputService.WindowFocusReleased:Connect(function()
    resetMovement()
end)

UserInputService.WindowFocused:Connect(function()
    resetMovement()
end)

-- –§—É–Ω–∫—Ü–∏—è –∞–≤—Ç–æ–ø–æ–¥–±–æ—Ä–∞ –Ω–∏–∫–∞
local function autoCompletePlayerName(textBox)
    local currentText = textBox.Text
    
    if currentText ~= "" then
        -- –ò—â–µ–º –∏–≥—Ä–æ–∫–æ–≤ –ø–æ —á–∞—Å—Ç–∏—á–Ω–æ–º—É —Å–æ–≤–ø–∞–¥–µ–Ω–∏—é
        local foundPlayers = {}
        
        for _, otherPlayer in pairs(Players:GetPlayers()) do
            if otherPlayer ~= player then
                local nameLower = string.lower(otherPlayer.Name)
                local displayLower = string.lower(otherPlayer.DisplayName)
                local inputLower = string.lower(currentText)
                
                -- –ü—Ä–æ–≤–µ—Ä—è–µ–º —Å–æ–≤–ø–∞–¥–µ–Ω–∏–µ –≤ –Ω–∞—á–∞–ª–µ –∏–º–µ–Ω–∏
                if string.sub(nameLower, 1, #inputLower) == inputLower then
                    table.insert(foundPlayers, otherPlayer.Name)
                elseif string.sub(displayLower, 1, #inputLower) == inputLower then
                    table.insert(foundPlayers, otherPlayer.DisplayName)
                end
            end
        end
        
        -- –ï—Å–ª–∏ –Ω–∞—à–ª–∏ –∏–≥—Ä–æ–∫–æ–≤ - –∞–≤—Ç–æ–¥–æ–ø–æ–ª–Ω—è–µ–º
        if #foundPlayers > 0 then
            -- –°–æ—Ä—Ç–∏—Ä—É–µ–º –ø–æ –∞–ª—Ñ–∞–≤–∏—Ç—É
            table.sort(foundPlayers)
            
            -- –ë–µ—Ä–µ–º –ø–µ—Ä–≤–æ–≥–æ –ø–æ–¥—Ö–æ–¥—è—â–µ–≥–æ –∏–≥—Ä–æ–∫–∞
            local selectedPlayer = foundPlayers[1]
            
            -- –£—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º —Ç–µ–∫—Å—Ç
            textBox.Text = selectedPlayer
            
            return true
        else
            return false
        end
    end
    return false
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –ø—Ä–∏–º–µ–Ω–µ–Ω–∏—è ESP –∫ –∫–æ–Ω–∫—Ä–µ—Ç–Ω–æ–º—É –∏–≥—Ä–æ–∫—É
local espHighlights = {}

local function applyESPToPlayer(otherPlayer)
    if otherPlayer == player then return end
    
    -- –£–¥–∞–ª—è–µ–º —Å—Ç–∞—Ä—ã–π ESP –µ—Å–ª–∏ –µ—Å—Ç—å
    if espHighlights[otherPlayer] then
        espHighlights[otherPlayer]:Destroy()
        espHighlights[otherPlayer] = nil
    end
    
    if not espEnabled then return end
    
    -- –ü—Ä–æ–≤–µ—Ä—è–µ–º —É—Å–ª–æ–≤–∏—è ESP
    local shouldESP = false
    if espTarget == "" then
        shouldESP = true
    else
        shouldESP = string.lower(otherPlayer.Name) == string.lower(espTarget) or 
                   string.lower(otherPlayer.DisplayName) == string.lower(espTarget)
    end
    
    if shouldESP and otherPlayer.Character then
        local highlight = Instance.new("Highlight")
        highlight.Name = "SosisonESP"
        highlight.FillTransparency = 1
        highlight.OutlineColor = Color3.fromRGB(0, 100, 255)
        highlight.OutlineTransparency = 0
        highlight.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
        highlight.Parent = otherPlayer.Character
        espHighlights[otherPlayer] = highlight
    end
end

-- –§—É–Ω–∫—Ü–∏—è –ø–æ–ª–Ω–æ–≥–æ –æ—Ç–∫–ª—é—á–µ–Ω–∏—è ESP
local function disableAllESP()
    espEnabled = false
    for player, highlight in pairs(espHighlights) do
        if highlight then
            highlight:Destroy()
        end
    end
    espHighlights = {}
end

-- –û–±—Ä–∞–±–æ—Ç—á–∏–∫ –ø–æ—è–≤–ª–µ–Ω–∏—è –Ω–æ–≤—ã—Ö –∏–≥—Ä–æ–∫–æ–≤
Players.PlayerAdded:Connect(function(newPlayer)
    if espEnabled then
        newPlayer.CharacterAdded:Connect(function(character)
            wait(1)
            applyESPToPlayer(newPlayer)
        end)
    end
end)

-- –û–±—Ä–∞–±–æ—Ç—á–∏–∫ —É—Ö–æ–¥–∞ –∏–≥—Ä–æ–∫–æ–≤
Players.PlayerRemoving:Connect(function(leftPlayer)
    if espHighlights[leftPlayer] then
        espHighlights[leftPlayer]:Destroy()
        espHighlights[leftPlayer] = nil
    end
end)

-- –§—É–Ω–∫—Ü–∏—è ESP (–í–•) - –û–ü–¢–ò–ú–ò–ó–ò–†–û–í–ê–ù–ù–ê–Ø
local lastESPUpdate = 0
local function updateESP()
    if not espEnabled then
        disableAllESP()
        return
    end
    
    -- –û–±–Ω–æ–≤–ª—è–µ–º –∫–∞–∂–¥—ã–µ 3 —Å–µ–∫—É–Ω–¥—ã –¥–ª—è –æ–ø—Ç–∏–º–∏–∑–∞—Ü–∏–∏
    local currentTime = tick()
    if currentTime - lastESPUpdate < 3 then return end
    lastESPUpdate = currentTime
    
    for _, otherPlayer in pairs(Players:GetPlayers()) do
        if otherPlayer ~= player then
            applyESPToPlayer(otherPlayer)
        end
    end
end

-- –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è ESP –¥–ª—è —Å—É—â–µ—Å—Ç–≤—É—é—â–∏—Ö –∏–≥—Ä–æ–∫–æ–≤
for _, otherPlayer in pairs(Players:GetPlayers()) do
    if otherPlayer ~= player then
        otherPlayer.CharacterAdded:Connect(function(character)
            if espEnabled then
                wait(1)
                applyESPToPlayer(otherPlayer)
            end
        end)
    end
end

-- –§—É–Ω–∫—Ü–∏—è –ø–ª–æ—Ç–Ω–æ–π —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏–∏ –≤ –∏–≥—Ä–æ–∫–∞
local function ultraDenseTeleport()
    if not enabled then return end
    
    local myChar = player.Character
    if not myChar then return end
    
    local myHRP = myChar:FindFirstChild("HumanoidRootPart")
    if not myHRP then return end
    
    local myPos = myHRP.Position
    
    if teleportAll then
        for _, otherPlayer in pairs(Players:GetPlayers()) do
            if otherPlayer ~= player then
                if ignorePlayerName ~= "" then
                    local shouldIgnore = string.lower(otherPlayer.Name) == string.lower(ignorePlayerName) or 
                                       string.lower(otherPlayer.DisplayName) == string.lower(ignorePlayerName)
                    if shouldIgnore then
                        continue
                    end
                end
                
                local otherChar = otherPlayer.Character
                if otherChar then
                    local otherHRP = otherChar:FindFirstChild("HumanoidRootPart")
                    if otherHRP then
                        local offset = Vector3.new(math.random(-1, 1), 0, math.random(-1, 1))
                        otherHRP.CFrame = CFrame.new(myPos + offset)
                    end
                end
            end
        end
    else
        if targetPlayerName ~= "" then
            local playersToTeleport = {}
            
            for name in string.gmatch(targetPlayerName, "[^,]+") do
                local trimmedName = string.gsub(name, "^%s*(.-)%s*$", "%1")
                if trimmedName ~= "" then
                    table.insert(playersToTeleport, trimmedName)
                end
            end
            
            for _, targetName in ipairs(playersToTeleport) do
                for _, otherPlayer in pairs(Players:GetPlayers()) do
                    if otherPlayer ~= player then
                        if string.lower(otherPlayer.Name) == string.lower(targetName) or 
                           string.lower(otherPlayer.DisplayName) == string.lower(targetName) then
                            local otherChar = otherPlayer.Character
                            if otherChar then
                                local otherHRP = otherChar:FindFirstChild("HumanoidRootPart")
                                if otherHRP then
                                    local offset = Vector3.new(math.random(-1, 1), 0, math.random(-1, 1))
                                    otherHRP.CFrame = CFrame.new(myPos + offset)
                                end
                            end
                        end
                    end
                end
            end
        end
    end
end

-- –§—É–Ω–∫—Ü–∏—è —É–ø—Ä–∞–≤–ª–µ–Ω–∏—è —Å–∫–æ—Ä–æ—Å—Ç—å—é
local function updateSpeedSystem()
    if not speedApplied then return end
    
    -- –ù–∞–¥–µ–∂–Ω–∞—è –ø—Ä–æ–≤–µ—Ä–∫–∞ –Ω–∞ –∞–∫—Ç–∏–≤–Ω—ã–π —á–∞—Ç
    if UserInputService:GetFocusedTextBox() ~= nil then
        resetMovement()
        return
    end
    
    local char = player.Character
    if not char or not char:FindFirstChild("HumanoidRootPart") then return end
    
    local hrp = char.HumanoidRootPart
    
    local oldBv = hrp:FindFirstChild("RePozSpeed")
    if oldBv then
        oldBv:Destroy()
    end
    
    if moveDirection.Magnitude > 0 then
        local newBv = Instance.new("BodyVelocity")
        newBv.Name = "RePozSpeed"
        
        local camera = workspace.CurrentCamera
        local cameraCFrame = camera.CFrame
        
        local right = cameraCFrame.RightVector * moveDirection.X
        local forward = cameraCFrame.LookVector * -moveDirection.Z
        local worldDirection = (right + forward) * customWalkSpeed
        
        worldDirection = Vector3.new(worldDirection.X, 0, worldDirection.Z)
        
        newBv.Velocity = worldDirection
        newBv.MaxForce = Vector3.new(40000, 0, 40000)
        newBv.Parent = hrp
    end
end

-- BodyVelocity –º–µ—Ç–æ–¥ –¥–ª—è –ø—Ä—ã–∂–∫–∞
local lastJumpUpdate = 0
local function updateJumpSystem()
    if not speedApplied then return end
    
    local currentTime = tick()
    if currentTime - lastJumpUpdate < 0.5 then return end
    
    local character = player.Character
    if not character or not character:FindFirstChild("HumanoidRootPart") then return end
    
    local humanoid = character:FindFirstChildOfClass("Humanoid")
    local hrp = character.HumanoidRootPart
    
    if humanoid and humanoid:GetState() == Enum.HumanoidStateType.Jumping then
        local speedBv = hrp:FindFirstChild("RePozSpeed")
        if speedBv then
            speedBv:Destroy()
        end
        
        local jumpBv = Instance.new("BodyVelocity")
        jumpBv.Name = "RePozJump"
        jumpBv.Velocity = Vector3.new(0, customJumpPower, 0)
        jumpBv.MaxForce = Vector3.new(0, 45000, 0)
        jumpBv.Parent = hrp
        
        lastJumpUpdate = currentTime
        
        delay(0.3, function()
            if jumpBv and jumpBv.Parent then
                jumpBv:Destroy()
            end
            if moveDirection.Magnitude > 0 then
                updateSpeedSystem()
            end
        end)
    end
end

-- –û–±—Ä–∞–±–æ—Ç—á–∏–∫–∏ –¥–≤–∏–∂–µ–Ω–∏—è —Å –Ω–∞–¥–µ–∂–Ω–æ–π –ø—Ä–æ–≤–µ—Ä–∫–æ–π —á–∞—Ç–∞
local function handleInput(input, isBegan)
    if UserInputService:GetFocusedTextBox() ~= nil then
        return
    end
    
    local key = input.KeyCode
    local direction = Vector3.new(0, 0, 0)
    
    if key == Enum.KeyCode.W then
        direction = Vector3.new(0, 0, -1)
    elseif key == Enum.KeyCode.S then
        direction = Vector3.new(0, 0, 1)
    elseif key == Enum.KeyCode.A then
        direction = Vector3.new(-1, 0, 0)
    elseif key == Enum.KeyCode.D then
        direction = Vector3.new(1, 0, 0)
    else
        return
    end
    
    if isBegan then
        moveDirection = moveDirection + direction
        lastInputTime = tick()
    else
        moveDirection = moveDirection - direction
        local char = player.Character
        if char and char:FindFirstChild("HumanoidRootPart") then
            local bv = char.HumanoidRootPart:FindFirstChild("RePozSpeed")
            if bv then
                bv:Destroy()
            end
        end
    end
end

UserInputService.InputBegan:Connect(function(input)
    handleInput(input, true)
end)

UserInputService.InputEnded:Connect(function(input)
    handleInput(input, false)
end)

-- –î–æ–ø–æ–ª–Ω–∏—Ç–µ–ª—å–Ω–∞—è –∑–∞—â–∏—Ç–∞: –æ—á–∏—Å—Ç–∫–∞ —Å–∫–æ—Ä–æ—Å—Ç–∏ –µ—Å–ª–∏ –Ω–µ—Ç –≤–≤–æ–¥–∞
RunService.Heartbeat:Connect(function()
    if tick() - lastInputTime > 0.1 and moveDirection.Magnitude == 0 then
        local char = player.Character
        if char and char:FindFirstChild("HumanoidRootPart") then
            local bv = char.HumanoidRootPart:FindFirstChild("RePozSpeed")
            if bv then
                bv:Destroy()
            end
        end
    end
end)

-- ========== –°–ò–°–¢–ï–ú–ê –ü–û–õ–ï–¢–ê ==========
local function startFlying()
    if isFlying then return end
    
    local character = player.Character
    if not character or not character:FindFirstChild("HumanoidRootPart") then return end
    
    isFlying = true
    local humanoidRootPart = character.HumanoidRootPart
    
    -- –°–æ–∑–¥–∞–µ–º BodyVelocity –¥–ª—è –ø–æ–ª–µ—Ç–∞
    flyBodyVelocity = Instance.new("BodyVelocity")
    flyBodyVelocity.Velocity = Vector3.new(0, 0, 0)
    flyBodyVelocity.MaxForce = Vector3.new(40000, 40000, 40000)
    flyBodyVelocity.Parent = humanoidRootPart
    
    -- –û–±—Ä–∞–±–æ—Ç–∫–∞ —É–ø—Ä–∞–≤–ª–µ–Ω–∏—è –ø–æ–ª–µ—Ç–æ–º
    flyConnection = RunService.Heartbeat:Connect(function()
        if not isFlying or not flyBodyVelocity or not humanoidRootPart then
            if flyConnection then
                flyConnection:Disconnect()
            end
            return
        end
        
        -- –ù–∞–¥–µ–∂–Ω–∞—è –ø—Ä–æ–≤–µ—Ä–∫–∞ –Ω–∞ –∞–∫—Ç–∏–≤–Ω—ã–π —á–∞—Ç
        if UserInputService:GetFocusedTextBox() ~= nil then
            flyBodyVelocity.Velocity = Vector3.new(0, 0, 0)
            return
        end
        
        local direction = Vector3.new(0, 0, 0)
        
        -- –ü–æ–ª—É—á–∞–µ–º –Ω–∞–ø—Ä–∞–≤–ª–µ–Ω–∏–µ –æ—Ç –∫–∞–º–µ—Ä—ã
        local camera = workspace.CurrentCamera
        local cameraCFrame = camera.CFrame
        
        -- W - –≤–ø–µ—Ä–µ–¥ (–ø–æ –Ω–∞–ø—Ä–∞–≤–ª–µ–Ω–∏—é –≤–∑–≥–ª—è–¥–∞ –∫–∞–º–µ—Ä—ã)
        if UserInputService:IsKeyDown(Enum.KeyCode.W) then
            direction = direction + cameraCFrame.LookVector
        end
        
        -- S - –Ω–∞–∑–∞–¥ (–ø—Ä–æ—Ç–∏–≤ –Ω–∞–ø—Ä–∞–≤–ª–µ–Ω–∏—è –≤–∑–≥–ª—è–¥–∞ –∫–∞–º–µ—Ä—ã)
        if UserInputService:IsKeyDown(Enum.KeyCode.S) then
            direction = direction - cameraCFrame.LookVector
        end
        
        -- A - –≤–ª–µ–≤–æ (–≤–ª–µ–≤–æ –æ—Ç –Ω–∞–ø—Ä–∞–≤–ª–µ–Ω–∏—è –∫–∞–º–µ—Ä—ã)
        if UserInputService:IsKeyDown(Enum.KeyCode.A) then
            direction = direction - cameraCFrame.RightVector
        end
        
        -- D - –≤–ø—Ä–∞–≤–æ (–≤–ø—Ä–∞–≤–æ –æ—Ç –Ω–∞–ø—Ä–∞–≤–ª–µ–Ω–∏—è –∫–∞–º–µ—Ä—ã)
        if UserInputService:IsKeyDown(Enum.KeyCode.D) then
            direction = direction + cameraCFrame.RightVector
        end
        
        -- Space - –≤–≤–µ—Ä—Ö
        if UserInputService:IsKeyDown(Enum.KeyCode.Space) then
            direction = direction + Vector3.new(0, 1, 0)
        end
        
        -- LeftShift - –≤–Ω–∏–∑
        if UserInputService:IsKeyDown(Enum.KeyCode.LeftShift) then
            direction = direction + Vector3.new(0, -1, 0)
        end
        
        -- –ù–æ—Ä–º–∞–ª–∏–∑—É–µ–º –Ω–∞–ø—Ä–∞–≤–ª–µ–Ω–∏–µ –∏ –ø—Ä–∏–º–µ–Ω—è–µ–º —Å–∫–æ—Ä–æ—Å—Ç—å
        if direction.Magnitude > 0 then
            flyBodyVelocity.Velocity = direction.Unit * flySpeed
        else
            flyBodyVelocity.Velocity = Vector3.new(0, 0, 0)
        end
    end)
end

local function stopFlying()
    if not isFlying then return end
    
    isFlying = false
    
    if flyBodyVelocity then
        flyBodyVelocity:Destroy()
        flyBodyVelocity = nil
    end
    
    if flyConnection then
        flyConnection:Disconnect()
        flyConnection = nil
    end
end

-- –û–±—Ä–∞–±–æ—Ç—á–∏–∫ –∫–ª–∞–≤–∏—à–∏ –¥–ª—è –ø–æ–ª–µ—Ç–∞
UserInputService.InputBegan:Connect(function(input, gameProcessed)
    if gameProcessed then return end
    
    local keyName = string.upper(input.KeyCode.Name)
    if keyName == flyBindKey then
        if isFlying then
            stopFlying()
        else
            startFlying()
        end
    end
end)

-- ========== –°–û–ó–î–ê–ù–ò–ï GUI ==========
