-- Обработчики автоподбора
autoCompleteSearchButton.MouseButton1Click:Connect(function()
    local success = autoCompletePlayerName(searchBox)
    if success then
        autoCompleteSearchButton.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
        wait(0.1)
        autoCompleteSearchButton.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
        
        -- Обновляем targetPlayerName
        targetPlayerName = searchBox.Text
        teleportAll = false
        button.Text = "ВКЛ Телепорт ВЫБРАННЫХ"
        statusLabel.Text = "Цель: " .. targetPlayerName
    else
        autoCompleteSearchButton.BackgroundColor3 = Color3.fromRGB(80, 30, 30)
        wait(0.2)
        autoCompleteSearchButton.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
    end
end)

autoCompleteIgnoreButton.MouseButton1Click:Connect(function()
    local success = autoCompletePlayerName(ignoreBox)
    if success then
        autoCompleteIgnoreButton.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
        wait(0.1)
        autoCompleteIgnoreButton.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
        
        -- Обновляем ignorePlayerName
        ignorePlayerName = ignoreBox.Text
    else
        autoCompleteIgnoreButton.BackgroundColor3 = Color3.fromRGB(80, 30, 30)
        wait(0.2)
        autoCompleteIgnoreButton.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
    end
end)

autoCompleteESPButton.MouseButton1Click:Connect(function()
    local success = autoCompletePlayerName(espSearchBox)
    if success then
        autoCompleteESPButton.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
        wait(0.1)
        autoCompleteESPButton.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
        
        -- Обновляем espTarget
        espTarget = espSearchBox.Text
        if espEnabled then
            espStatusLabel.Text = "ВХ: ВКЛ - " .. espTarget
            -- Обновляем ESP для новой цели
            for _, otherPlayer in pairs(Players:GetPlayers()) do
                if otherPlayer ~= player then
                    applyESPToPlayer(otherPlayer)
                end
            end
        end
    else
        autoCompleteESPButton.BackgroundColor3 = Color3.fromRGB(80, 30, 30)
        wait(0.2)
        autoCompleteESPButton.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
    end
end)

-- Остальные обработчики
searchBox:GetPropertyChangedSignal("Text"):Connect(function()
    if searchBox.Text == "" then
        teleportAll = true
        targetPlayerName = ""
        button.Text = "ВКЛ Телепорт ВСЕХ"
    else
        teleportAll = false
        targetPlayerName = searchBox.Text
        button.Text = "ВКЛ Телепорт ВЫБРАННЫХ"
    end
    statusLabel.Text = "Цель: " .. (targetPlayerName == "" and "Все" or targetPlayerName)
end)

ignoreBox:GetPropertyChangedSignal("Text"):Connect(function()
    ignorePlayerName = ignoreBox.Text
end)

button.MouseButton1Click:Connect(function()
    enabled = not enabled
    if enabled then
        button.Text = "ВЫКЛ Телепорт " .. (teleportAll and "ВСЕХ" or "ВЫБРАННЫХ")
        button.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
        print("Project Sosison телепорт включен")
    else
        button.Text = "ВКЛ Телепорт " .. (teleportAll and "ВСЕХ" or "ВЫБРАННЫХ")
        button.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
        print("Project Sosison телепорт выключен")
    end
end)

-- Обработчики ESP
espSearchBox:GetPropertyChangedSignal("Text"):Connect(function()
    espTarget = espSearchBox.Text
    if espEnabled then
        espStatusLabel.Text = "ВХ: ВКЛ - " .. (espTarget == "" and "Все игроки" or espTarget)
        -- Обновляем ESP при изменении цели
        for _, otherPlayer in pairs(Players:GetPlayers()) do
            if otherPlayer ~= player then
                applyESPToPlayer(otherPlayer)
            end
        end
    end
end)

espAllButton.MouseButton1Click:Connect(function()
    espEnabled = true
    espStatusLabel.Text = "ВХ: ВКЛ - " .. (espTarget == "" and "Все игроки" or espTarget)
    espAllButton.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
    espOffButton.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
    print("Project Sosison WH включен")
    
    -- Немедленно применяем ESP ко всем игрокам
    for _, otherPlayer in pairs(Players:GetPlayers()) do
        if otherPlayer ~= player then
            applyESPToPlayer(otherPlayer)
        end
    end
end)

espOffButton.MouseButton1Click:Connect(function()
    disableAllESP()
    espStatusLabel.Text = "ВХ: ВЫКЛ"
    espAllButton.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
    espOffButton.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
    print("Project Sosison WH выключен")
end)

-- Обработчики вкладок
local function showTab(tab)
    teleportContent.Visible = (tab == "teleport")
    speedContent.Visible = (tab == "speed") 
    espContent.Visible = (tab == "esp")
    
    teleportTab.BackgroundColor3 = (tab == "teleport") and Color3.fromRGB(25, 25, 25) or Color3.fromRGB(15, 15, 15)
    speedTab.BackgroundColor3 = (tab == "speed") and Color3.fromRGB(25, 25, 25) or Color3.fromRGB(15, 15, 15)
    espTab.BackgroundColor3 = (tab == "esp") and Color3.fromRGB(25, 25, 25) or Color3.fromRGB(15, 15, 15)
    
    teleportTab.TextColor3 = (tab == "teleport") and Color3.fromRGB(200, 200, 200) or Color3.fromRGB(150, 150, 150)
    speedTab.TextColor3 = (tab == "speed") and Color3.fromRGB(200, 200, 200) or Color3.fromRGB(150, 150, 150)
    espTab.TextColor3 = (tab == "esp") and Color3.fromRGB(200, 200, 200) or Color3.fromRGB(150, 150, 150)
end

teleportTab.MouseButton1Click:Connect(function()
    showTab("teleport")
end)

speedTab.MouseButton1Click:Connect(function()
    showTab("speed")
end)

espTab.MouseButton1Click:Connect(function()
    showTab("esp")
end)

applySpeedButton.MouseButton1Click:Connect(function()
    local newSpeed = tonumber(speedBox.Text)
    local newJump = tonumber(jumpBox.Text)
    
    if newSpeed then
        customWalkSpeed = newSpeed
        speedLabel.Text = "Скорость бега: " .. newSpeed
    end
    
    if newJump then
        customJumpPower = newJump
        jumpLabel.Text = "Высота прыжка: " .. newJump
    end
    
    speedApplied = true
    print("Project Sosison настройки скорости применены")
end)

emergencyStop.MouseButton1Click:Connect(function()
    resetMovement()
    speedApplied = false
    print("Project Sosison экстренная остановка")
end)

-- Скрытие/показ GUI по правому CTRL
UserInputService.InputBegan:Connect(function(input)
    if input.KeyCode == Enum.KeyCode.RightControl then
        guiVisible = not guiVisible
        mainFrame.Visible = guiVisible
    end
end)

closeButton.MouseButton1Click:Connect(function()
    screenGui:Destroy()
    enabled = false
    disableAllESP()
    print("Project Sosison выключен")
end)

-- Добавляем GUI в игрока
screenGui.Parent = player.PlayerGui

print(" Автоподбор активирован!")
print(" Напиши часть ника и нажми кнопку АВТОПОДБОР")

-- Циклы обновления
RunService.Heartbeat:Connect(function()
    ultraDenseTeleport()
    updateSpeedSystem()
    updateJumpSystem()
    updateESP()
end)
